{{# each produtos}}
<div class="row">
  {{# each this}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{ this.imagePath}}" alt="..." class="img-responsive">
      <div class="caption">
        <h3>{{ this.titulo}}</h3>
        <p class="descricao">{{ this.descricao}}</p>

        <div class="clearfix">
          <div class="preco pull-left">{{ this.preco}}</div>
          <a href="#" class="btn btn-success pull-right" role="button">Button</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}